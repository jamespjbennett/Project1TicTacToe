%p.notice= notice
%p.alert= alert
- if user_signed_in?
  %li.signup-list= link_to('Logout', destroy_user_session_path, :method => :delete)
  %li.signup-list= link_to('Edit registration', edit_user_registration_path)
- else
  %li.signup-list= link_to('Login', new_user_session_path)
  %li.signup-list= link_to('Register', new_user_registration_path)
    