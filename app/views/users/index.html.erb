<% @users.order("wins desc").each do |user| %>
  <table class='user-table'>
    <% if user.wins != nil %>
      <th>Email</th>
      <th>Number of Wins</th>
      <th>Number of Losses</th>
      <th>Number of Draws</th>
      <th>Wins percentage</th>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.wins %></td>
        <td><%= user.losses %></td>
        <td><%= user.draws %></td>
      </tr>
    <% end %>
  </table>
<% end %>


